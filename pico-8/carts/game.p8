-- main

#include MicroQiskit.lua
items = {sword="h",shield="cx",ring="pi/4"}
function _init()
	game_over=false
	make_player()
	--make_ground()
end

function damage_calculations()
    x=0
    local qc = QuantumCircuit()
    qc.set_registers(4)
    --check what items the player is holding and do the gate
    qc.h(0)
    qc.h(1)
    qc.h(2)
    qc.h(3)
    qc.cx(0,1)
    qc.cx(0,2)
    qc.cx(0,3)

    local meas = QuantumCircuit()
    meas.set_registers(4,4)
    meas.measure(0,0)
    meas.measure(1,1)
    meas.measure(3,3)
    meas.measure(2,2)

    qc.add_circuit(meas)
    result = simulate(qc,"counts",100)
    print("\nThe counts are\n",55,55)
    for string, counts in pairs(result) do
        print(string.."="..counts)
    end

end


function _update()
	move_player()
end

function _draw()
	cls()
	--draw_ground()
	draw_player()
	damage_calculations()
end

function rndb(low,high)
	return flr(rnd(high-low+1)+low)
end


